{
  "routes": [
    {
      "method": "GET",
      "path": "/list",
      "controller": "list.find",
      "config": {
        "middleware": ["isLoggedIn"]
      }
    },
    {
      "method": "GET",
      "path": "/list/count",
      "controller": "list.count",
      "config": {
        "middleware": []
      }
    },
    {
      "method": "GET",
      "path": "/list/:id",
      "controller": "list.findOne",
      "config": {
        "middleware": []
      }
    },
    {
      "method": "POST",
      "path": "/list",
      "controller": "list.create",
      "config": {
        "middleware": []
      }
    },
    {
      "method": "PUT",
      "path": "/list/:id",
      "controller": "list.update",
      "config": {
        "middleware": []
      }
    },
    {
      "method": "DELETE",
      "path": "/list/:id",
      "controller": "list.delete",
      "config": {
        "middleware": []
      }
    }
  ]
}